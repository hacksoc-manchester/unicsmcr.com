<!DOCTYPE html>
<html lang="en">

<head>
  <% include ../partials/head.ejs %>
    <title>Team | UniCS</title>
    <link rel="stylesheet" type="text/css" href="css/team.css" />
</head>

<body>
  <div id="container">
    <% include ../partials/navbar.ejs %>
    <% include ../partials/header.ejs %>
      <div id="body">
          <section id="team">
              <h1>Team</h1>
              <div class="members">
                <% team.forEach(member => { %>
                  <div class="member" style="background-image: url('images/team/<%= member.id %>.small.jpg')">
                    <div>
                      <h2><%= member.name %></h2>
                      <p><%= member.description %></p>
                    </div>
                  </div>
                <% }) %>
              </div>
              <div class="separator"></div>
              <h1>Hall of Fame</h1>
              <div class="members">
                <% hallOfFame.presidents.forEach(member => { %>
                  <div class="member" style="background-image: url('images/team/<%= member.id %>.small.jpg')">
                    <div>
                      <h2><%= member.name %></h2>
                      <p><%= member.description %></p>
                    </div>
                  </div>
                <% }) %>
              </div>
              <button id="seeMoreButton" class="btn btn-outline-dark" type="button" data-toggle="collapse" data-target="#otherMembers" aria-expanded="false" aria-controls="otherMembers">
                See More
              </button>
              <div class="members collapse" id="otherMembers">
                <% hallOfFame.other.forEach(member => { %>
                  <div class="member" style="background-image: url('images/team/<%= member.id %>.small.jpg')">
                    <div>
                      <h2><%= member.name %></h2>
                      <p><%= member.description %></p>
                    </div>
                  </div>
                <% }) %>
              </div>
            </section>
      </div>
      <% include ../partials/footer.ejs %>
  </div>
</body>

</html>